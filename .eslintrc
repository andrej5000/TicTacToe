// 0: off, 1: warning, 2: error
{
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "globals": {
        "Model": false
    },
    "rules": {
        "react/display-name": 1,
        "jsx-quotes": [
            1,
            "prefer-single"
        ],
        "react/jsx-no-undef": 1,
        "react/jsx-uses-react": 1,
        "react/jsx-uses-vars": 1,
        "react/no-did-mount-set-state": 0,
        "react/no-did-update-set-state": 1,
        "react/no-multi-comp": 1,
        "react/no-unknown-property": 1,
        "react/prop-types": 1,
        "react/react-in-jsx-scope": 1,
        "react/self-closing-comp": 1,
        "react/jsx-wrap-multilines": 1,
        "comma-dangle": 2,
        "no-cond-assign": [
            2,
            "except-parens"
        ],
        "no-console": 0,
        "no-constant-condition": 2,
        "no-control-regex": 2,
        "no-debugger": 2,
        "no-dupe-keys": 2,
        "no-empty": 2,
        "no-empty-character-class": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": 0,
        "no-extra-semi": 2,
        "no-func-assign": 2,
        "no-inner-declarations": 2,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-obj-calls": 2,
        "no-regex-spaces": 2,
        "no-sparse-arrays": 2,
        "no-unreachable": 2,
        "use-isnan": 2,
        "valid-jsdoc": [
            0,
            {
                "requireReturn": false
            }
        ],
        "valid-typeof": 2,
        "block-scoped-var": 0,
        "complexity": [
            1,
            20
        ],
        "consistent-return": 0,
        "curly": [
            2,
            "all"
        ],
        "default-case": 2,
        "dot-notation": [
            2,
            {
                "allowKeywords": true
            }
        ],
        "eqeqeq": 2,
        "guard-for-in": 1,
        "no-alert": 1,
        "no-caller": 2,
        "no-div-regex": 1,
        "no-else-return": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-extra-bind": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-implied-eval": 2,
        "no-iterator": 2,
        "no-labels": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-multi-spaces": 1,
        "no-multi-str": 2,
        "no-native-reassign": 2,
        "no-new": 2,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-octal": 2,
        "no-octal-escape": 2,
        "no-process-env": 0,
        "no-proto": 2,
        "no-redeclare": 2,
        "no-return-assign": 2,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-unused-expressions": 0,
        "no-void": 2,
        "no-with": 2,
        "radix": 2,
        "vars-on-top": 0,
        "wrap-iife": [
            2,
            "inside"
        ],
        "yoda": [
            2,
            "never"
        ],
        "strict": [
            0,
            "never"
        ],
        "no-catch-shadow": 2,
        "no-delete-var": 2,
        "no-label-var": 2,
        "no-shadow": 0,
        "no-shadow-restricted-names": 2,
        "no-undef": 0,
        "no-undef-init": 2,
        "no-undefined": 0,
        "no-unused-vars": [
            1,
            {
                "vars": "all",
                "args": "after-used"
            }
        ],
        "no-use-before-define": [
            1,
            "nofunc"
        ],
        "handle-callback-err": 1,
        "no-mixed-requires": 0,
        "no-new-require": 2,
        "no-path-concat": 2,
        "no-process-exit": 1,
        "no-restricted-modules": 0,
        "no-sync": 0,
        "indent": 0,
        //["error", 4],
        "brace-style": [
            1,
            "1tbs",
            {
                "allowSingleLine": false
            }
        ],
        "camelcase": 0,
        "comma-spacing": [
            1,
            {
                "before": false,
                "after": true
            }
        ],
        "comma-style": [
            2,
            "last"
        ],
        "consistent-this": [
            2,
            "self"
        ],
        "eol-last": 1,
        "func-names": 0,
        "func-style": 0,
        "key-spacing": [
            1,
            {
                "beforeColon": false,
                "afterColon": true
            }
        ],
        "max-nested-callbacks": [
            2,
            7
        ],
        "new-cap": [
            1,
            {
                "capIsNew": false
            }
        ],
        "new-parens": 2,
        "no-array-constructor": 2,
        "no-inline-comments": 0,
        "no-lonely-if": 1,
        "no-mixed-spaces-and-tabs": 1,
        "no-multiple-empty-lines": [
            1,
            {
                "max": 3
            }
        ],
        "no-nested-ternary": 0,
        "no-new-object": 2,
        "semi-spacing": 1,
        "no-spaced-func": 1,
        "no-ternary": 0,
        "no-trailing-spaces": 1,
        "no-underscore-dangle": 0,
        "one-var": 0,
        "operator-assignment": 0,
        "padded-blocks": [
            0,
            "always"
        ],
        "quote-props": [
            1,
            "as-needed"
        ],
        "quotes": [
            1,
            "single",
            "avoid-escape"
        ],
        "semi": [
            2,
            "always"
        ],
        "sort-vars": 0,
        "space-before-function-paren": [
            1,
            {
                "anonymous": "always",
                "named": "never"
            }
        ],
        "keyword-spacing": 1,
        "space-before-blocks": [
            1,
            "always"
        ],
        "object-curly-spacing": [
            "error",
            "never"
        ],
        "array-bracket-spacing": [
            1,
            "never"
        ],
        "computed-property-spacing": [
            1,
            "never"
        ],
        "space-in-parens": [
            1,
            "never"
        ],
        "space-infix-ops": 1,
        "space-unary-ops": [
            2,
            {
                "words": true,
                "nonwords": false
            }
        ],
        "wrap-regex": 0,
        "no-var": 0,
        "max-depth": [
            1,
            6
        ],
        "max-params": [
            2,
            4
        ],
        "max-statements": [
            1,
            15
        ],
        "no-bitwise": 2,
        "no-plusplus": 0,
        "arrow-spacing": 1
    }
}
