{
    "collectCoverageFrom": [
        "./src/**"
    ],
    "coverageDirectory": "./target/coverage",
    "moduleNameMapper": {
        ".*\\.scss": "identity-obj-proxy"
    },
    "rootDir": "../..",
    "roots": [
        "./src",
        "./test"
    ],
    "setupFilesAfterEnv": [
        "<rootDir>/test/config/enzymeJestSetup.js"
    ],
    "testPathIgnorePatterns": [
        "\\\\node_modules\\\\"
    ],
    "testMatch": [
        "**/**/*Test.js"
    ],
    "verbose": true
}
